<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="275.0"  prefWidth="489.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.PrimaryController">
<padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Label fx:id="MyLabel1" alignment="TOP_CENTER" prefHeight="63.0" prefWidth="380.0" text="Select the shape you want" />
      <ChoiceBox fx:id="MyChoiceBox" prefHeight="25.0" prefWidth="145.0" />
      <Button fx:id="Enter1" alignment="BOTTOM_CENTER" mnemonicParsing="false" onAction="#SwitchToShape" text="Enter" />
   </children>

</VBox>



   
