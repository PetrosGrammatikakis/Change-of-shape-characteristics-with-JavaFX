<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Cylinder?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0"  xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.CylinderController">
   <children>
      <Button fx:id="Back" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#Back5" text="&lt;--" />
      <Cylinder fx:id="Cylinder1" cullFace="FRONT" height="100.0" layoutX="300.0" layoutY="230.0" radius="20.0" />
      <Button fx:id="Enter" layoutX="286.0" layoutY="27.0" mnemonicParsing="false" onAction="#CylinderShape" text="Enter" />
      <TextField fx:id="Height" layoutX="137.0" layoutY="21.0" prefHeight="31.0" prefWidth="149.0" promptText="Height" />
      <TextField fx:id="Radius" layoutX="329.0" layoutY="21.0" prefHeight="31.0" prefWidth="149.0" promptText="Radius" />
   </children>
</AnchorPane>